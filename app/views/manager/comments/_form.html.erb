<%= form_with(model: [@client, @project, @time_log], local: true) do |form| %>
  <div class="form-group">
    <%= form.label :hours %>
    <%= form.number_field :hours, class: "form-control", required: true %>
  </div>

  <div class="form-group">
    <%= form.submit "Save Time Log", class: "btn btn-primary" %>
  </div>
<% end %>
